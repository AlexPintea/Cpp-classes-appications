\newcommand{\fakesection}[1]{
  \par\refstepcounter{section}
  \sectionmark{#1}
  \addcontentsline{toc}{section}{\largecontents\protect\numberline{}#1\vspace{6pt}}
}
\newcommand{\fakesubsection}[1]{
  \par\refstepcounter{subsection}
  \sectionmark{#1}
  \addcontentsline{toc}{subsection}{\smallcontents\protect\numberline{}#1\vspace{4pt}}
  
}
